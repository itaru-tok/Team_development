<% if movie.genre.include?("rails")%>
  <div class="col">
    <div class="card">
      <iframe src="<%= movie.url %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
      <div class="card-body">
        <a href="#" class="btn btn-primary btn-block">読破済みにする</a>
        <p class="card-title">Lv.<%= movie.id %>：<%= movie.title%></p>
      </div>
    </div>
  </div>
<% end %>